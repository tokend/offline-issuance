{
  "name": "offline-issuance",
  "version": "0.1.0",
  "description": "TokenD offline issuance tool based on electron-vue",
  "scripts": {
    "build": "node tasks/release.js",
    "build:clean": "cross-env PLATFORM_TARGET=clean node tasks/release.js",
    "build:darwin": "cross-env PLATFORM_TARGET=darwin node tasks/release.js",
    "build:linux": "cross-env PLATFORM_TARGET=linux node tasks/release.js",
    "build:mas": "cross-env PLATFORM_TARGET=mas node tasks/release.js",
    "build:win32": "cross-env PLATFORM_TARGET=win32 node tasks/release.js",
    "dev": "node tasks/runner.js",
    "make": "node make.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app test",
    "pack": "yarn pack:main && yarn pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.renderer.config.js",
    "unit": "cross-env BABEL_ENV=testing-unit karma start test/unit/karma.conf.js"
  },
  "author": "Distributed Lab <dev@distributedlab.com>",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.1",
    "babel-plugin-istanbul": "3.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "cross-env": "3.2.4",
    "css-loader": "0.26.4",
    "del": "2.2.2",
    "devtron": "1.4.0",
    "electron": "1.8.8",
    "electron-debug": "1.5.0",
    "electron-devtools-installer": "2.2.3",
    "electron-packager": "8.7.2",
    "electron-rebuild": "1.7.3",
    "eslint": "3.19.0",
    "eslint-config-standard": "6.2.1",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "2.0.3",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "2.3.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.30.1",
    "inject-loader": "2.0.1",
    "json-loader": "0.5.7",
    "node-sass": "4.7.2",
    "require-dir": "0.3.2",
    "sass-loader": "4.1.1",
    "spectron": "3.8.0",
    "style-loader": "0.13.2",
    "tree-kill": "1.2.0",
    "url-loader": "0.5.9",
    "vue-hot-reload-api": "2.3.0",
    "vue-html-loader": "1.2.4",
    "vue-loader": "10.3.0",
    "vue-style-loader": "1.0.0",
    "vue-template-compiler": "2.5.13",
    "webpack": "2.7.0",
    "webpack-dev-server": "2.11.2",
    "webpack-merge": "2.6.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.3.1",
    "bootstrap-sass": "3.3.7",
    "electron-winstaller": "2.6.4",
    "exports-loader": "0.6.4",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "node-sass": "4.7.2",
    "randomstring": "1.1.5",
    "stellar-wallet-js-sdk": "0.0.0"
  }
}
